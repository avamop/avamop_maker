{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./types",
    "noEmit": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "traceResolution": true,
    "downlevelIteration":true,
    "moduleResolution": "node",
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"]
  },
  "include": [
    "src/lib"
  ]
}
